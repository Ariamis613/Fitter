{
  "tasks": [
      {
          "type": "shell",
          "label": "g++ build active file",
          "command": "/usr/bin/g++",
          "args": [
              "-g",
              "-Wall",
              "-std=c++23",
              "${file}",
              "-o",
              "${fileDirname}/${fileBasenameNoExtension}"
          ],
          "options": {
              // "cwd": "/usr/bin"
              "cwd": "${workspaceFolder}"
          }
      }
  ],
  "version": "2.0.0"
}
